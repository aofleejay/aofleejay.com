{
  "name": "aofleejay.com",
  "private": true,
  "description": "Just another blog",
  "version": "0.1.1",
  "author": "aofleejay",
  "scripts": {
    "build": "gatsby build",
    "dev": "gatsby develop",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "format": "prettier --ignore-path .gitignore \"**/*.+(js|json|ts|tsx)\" --write",
    "lint": "eslint --ignore-path .gitignore --ext .js,.jsx,.ts,.tsx src",
    "test": "jest",
    "test:watch": "jest --watch",
    "cypress:open": "cypress open",
    "coveralls": "jest --coverage && cat ./coverage/lcov.info | coveralls",
    "deploy": "gatsby build && gh-pages -d public",
    "create:blog": "ts-node src/scripts/create-blog.ts",
    "resize:image": "ts-node src/scripts/resize-image.ts"
  },
  "dependencies": {
