    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-cypress": "^2.11.2",
    "gatsby-plugin-postcss": "^3.5.0",
    "gh-pages": "^3.1.0",
    "husky": "^4.3.8",
    "identity-obj-proxy": "^3.0.0",
    "inquirer": "^7.0.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
